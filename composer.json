{
    "name": "jalendport/craft-sidebarthemes",
    "description": "Customise the look of the CP sidebar",
    "type": "craft-plugin",
    "version": "1.0.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "sidebar themes"
    ],
    "support": {
        "docs": "https://github.com/jalendport/craft-sidebarthemes/blob/master/README.md",
        "issues": "https://github.com/jalendport/craft-sidebarthemes/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jalen Davenport",
            "homepage": "https://jalendport.com"
        },
        {
            "name": "Luke Youell",
            "homepage": "https://github.com/lukeyouell"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "jalendport\\sidebarthemes\\": "src/"
        }
    },
    "extra": {
        "name": "Sidebar Themes",
        "handle": "sidebar-themes",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/jalendport/craft-sidebarthemes/master/CHANGELOG.md",
        "class": "jalendport\\sidebarthemes\\SidebarThemes"
    }
}
